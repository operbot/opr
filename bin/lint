#!/usr/bin/env python3


import os
import sys


os.environ["PYTHONPATH"] = "."


CMD = "pylint --disable=R,C,E1101,W0201,W0212,W0613 %s"
SKIP = ["Your code", "---"]
TODO = ["opr", 'opm']


def popen(txt):
    for line in os.popen(txt).readlines():
        doskip = False
        for skip in SKIP:
            if skip in line:
                doskip = True
        if doskip:
            continue
        res = line.rstrip()
        if res:
            print(res)


for name in TODO:
    popen(CMD % name)
